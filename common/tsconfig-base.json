{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "jsxFactory": "JSXAlone.createElement",
    "declaration": true,
    "strict": true,  
    "composite": true,
    "sourceMap": true,
    "noStrictGenericChecks": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "allowSyntheticDefaultImports": false,
  }
}
